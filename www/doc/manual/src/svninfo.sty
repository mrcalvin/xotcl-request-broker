  \fi
  \@ifundefined{svnKeyword@#1\svn@suffix}%
      {\@nameuse{svnKeyword@generic\svn@suffix}{#1}{#2}}%
    {\@nameuse{svnKeyword@#1\svn@suffix}{#1}{#2}}%
}
